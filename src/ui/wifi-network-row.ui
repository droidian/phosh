<?xml version="1.0" encoding="utf-8"?>
<interface>
  <requires lib="gtk+" version="3.20"/>
  <template class="PhoshWifiNetworkRow" parent="HdyActionRow">
    <property name="visible">True</property>
    <property name="activatable">True</property>
    <child type="prefix">
      <object class="GtkBox">
        <property name="visible">1</property>
        <property name="valign">center</property>
        <child>
          <object class="GtkImage" id="wifi_icon">
            <property name="visible">1</property>
            <property name="icon-name">network-wireless-signal-none</property>
          </object>
        </child>
        <child>
          <object class="GtkImage" id="encrypted_icon">
            <property name="visible">1</property>
            <property name="valign">end</property>
            <property name="pixel-size">8</property>
            <property name="icon-name">network-wireless-encrypted</property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkImage" id="active_indicator">
        <property name="visible">1</property>
        <property name="icon-name">object-select</property>
      </object>
    </child>
  </template>
</interface>
